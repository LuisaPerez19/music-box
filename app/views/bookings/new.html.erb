
<%= form_for @booking do |f| %>
<div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
</div>

<div class="field">
    <%= f.label :email %>
    <%= f.email_field :email %>
</div>

<!-- Add other form fields as needed -->

<div class="actions">
    <%= f.submit "Create Booking" %>
</div>
<% end %>

<%= link_to 'Back to Bookings', bookings_path %>